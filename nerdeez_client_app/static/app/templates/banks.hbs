                        				</div> <!-- /field -->
                                        
                        				<div class="field">
                        					<label for="loans">הלוואות:</label>	
                        					{{view Ember.TextField class="login" placeholder="הלוואות" valueBinding="loans" required="true" name="loans"}}
                        				</div> <!-- /field -->
                                        
