                        				</div> <!-- /field -->
                                        
                        				<div class="field">
                        					<label for="phone">טלפון:</label>	
                        					{{view Ember.TextField class="login" placeholder="טלפון" valueBinding="phone" required="true" name="phone"}}
                        				</div> <!-- /field -->
                                        
                        			</div>
                                </div>
                            </div>
                        </li>
                        
